{
  "name": "vekony-lab",
  "version": "1.0.0",
  "description": "TaborProgram frontend",
  "scripts": {
    "install:client": "cd public && npm i",
    "build:server": "cd app && tsc",
    "start": "node app/start-server.js",
    "dev-server": "cd public && \"node_modules/.bin/webpack-dev-server\" --config webpack.config.dev.js --content-base dist",
    "build:client": "cd public && webpack --config webpack.config.prod.js",
    "watch:client": "cd public && webpack --config webpack.config.dev.js -w"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/socket.io": "^1.4.29",
    "body-parser": "^1.17.1",
    "config-decorators": "^0.1.3",
    "express": "^4.15.2",
    "modular-log": "^0.3.11",
    "socket.io": "^1.7.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/node": "^6.0.46",
    "typescript": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:igabesz/vekony-lab-ng2.git"
  }
}
